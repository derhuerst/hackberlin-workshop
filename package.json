{
  "private": true,
  "name": "hackberlin-workshop",
  "description": "A workshop on building frontend apps.",
  "version": "0.1.0",
  "keywords": [
    "hackathon",
    "workshop",
    "frontend",
    "demo",
    "berlin"
  ],
  "author": "Jannis R <mail@jannisr.de>",
  "homepage": "http://derhuerst.github.io/hackberlin-workshop/",
  "repository": "derhuerst/hackberlin-workshop",
  "bugs": "https://github.com/derhuerst/hackberlin-workshop/issues",
  "license": "ISC",
  "engines": {
    "node": ">=6"
  },
  "devDependencies": {
    "beefy": "^2.1.8",
    "browserify": "^14.0.0",
    "uglify-js": "^3.0.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "env"
          ],
          "global": true
        }
      ]
    ]
  },
  "scripts": {
    "bundle": "browserify index.js > bundle.js",
    "minify": "cat bundle.js | uglifyjs -mc > bundle.min.js",
    "build": "npm run bundle && npm run minify",
    "start": "beefy index.js:bundle.js 9966 --live"
  },
  "dependencies": {
    "babel-preset-env": "^1.6.0",
    "babelify": "^8.0.0",
    "global": "^4.3.1",
    "raf": "^3.3.0",
    "random-color": "^1.0.1",
    "yo-yo": "^1.3.1"
  }
}
